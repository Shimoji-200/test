
public class Iphone {

	public static void main(String[] args) {
		SmartPhone iphone=new SmartPhone();
		iphone.play();
		iphone.stop();
		iphone.next();
		iphone.back();
		iphone.call();
		iphone.mail();
		iphone.photo();
		iphone.internet();}

		public void play(){
			System.out.println("再生(ハイレゾ機能)");
			}
			public void stop(){
			System.out.println("停止(ハイレゾ機能)");
			}
			public void next(){
			System.out.println("次へ(ハイレゾ機能)");
			}
			public void back(){
			System.out.println("戻る(ハイレゾ機能)");
	}

}
